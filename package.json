    "coveralls": "^2.11.15",
    "eslint": "~3.14.1",
    "eslint-config-muriki": "~8.1.4",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jasmine": "~2.2.0",
    "eslint-plugin-node": "~3.0.5",
    "husky": "~0.13.1",
    "jest": "^18.1.0",
    "jest-babel": "~1.0.1",
    "lint-staged": "~3.2.8",
    "lodash": "^4.17.4"
  }
